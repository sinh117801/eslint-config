{
  "name": "@sinh/eslint-config-monorepo",
  "version": "1.0.0",
  "description": "Sinh's ESLint config presets",
  "scripts": {
    "lint": "eslint .",
    "bump:prerelease": "bumpp prerelease package.json packages/*/package.json --preid $PREID -yes --no-verify",
    "bump:next": "bumpp next package.json packages/*/package.json --preid $PREID -yes --no-verify",
    "bump": "bumpp package.json packages/*/package.json --preid $PREID",
    "release": "pnpm -r publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sinh117801/eslint-config.git"
  },
  "keywords": [
    "eslint-config"
  ],
  "author": "Sinh <noun.sinh@icloud.com> (https://github.com/sinh117801/)",
  "license": "MIT",
  "packageManager": "pnpm@7.1.0",
  "dependencies": {
    "@sinh/eslint-config": "workspace:*",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "bumpp": "^8.2.1"
  },
  "bugs": {
    "url": "https://github.com/sinh117801/eslint-config/issues"
  },
  "homepage": "https://github.com/sinh117801/eslint-config#readme"
}
